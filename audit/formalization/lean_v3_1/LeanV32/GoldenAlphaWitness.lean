/-- Extend the finite vector to a full sequence by padding with zeros. -/
def goldenAlphaSeq (k : Nat) : Complex :=
  if h : k < 160 then
    Vector.get goldenAlphaVec (Fin.mk k h)
  else
    0

private theorem goldenAlphaVec_normSq_le (i : Fin 160) :
    Complex.normSq (Vector.get goldenAlphaVec i) <= goldenR0 ^ 2 := by
  fin_cases i <;> norm_num [goldenAlphaVec, goldenR0, Complex.normSq]

private theorem goldenAlphaVec_norm_le (i : Fin 160) :
    norm (Vector.get goldenAlphaVec i) <= goldenR0 := by
  have hsq : Complex.normSq (Vector.get goldenAlphaVec i) <= goldenR0 ^ 2 :=
    goldenAlphaVec_normSq_le (i := i)
  have hsq' : norm (Vector.get goldenAlphaVec i) ^ 2 <= goldenR0 ^ 2 := by
    simpa [Complex.normSq_eq_norm_sq] using hsq
  have hr0 : 0 <= goldenR0 := by norm_num [goldenR0]
  exact le_of_sq_le_sq hsq' hr0

/-- Kernel-checked instance of `AlphaRadiusBound` for the exported golden sequence. -/
theorem goldenAlphaRadiusBound : AlphaRadiusBound goldenAlphaSeq := by
  refine Exists.intro goldenR0 ?_
  refine And.intro (by norm_num [goldenR0]) ?_
  refine And.intro (by norm_num [goldenR0]) ?_
  intro k
  by_cases hk : k < 160
  case pos =>
    simpa [goldenAlphaSeq, hk] using goldenAlphaVec_norm_le (i := Fin.mk k hk)
  case neg =>
    have hr0 : 0 <= goldenR0 := by norm_num [goldenR0]
    simpa [goldenAlphaSeq, hk] using hr0

/-- Downstream consequence: denominator separation holds for every `z` in the upper half-plane. -/
theorem denomSepCocycleAt_golden (z : Complex) (hz : 0 < z.im) :
    DenomSepCocycleAt z goldenAlphaSeq := by
  exact denomSepCocycleAt_of_alphaRadiusBound z goldenAlphaSeq hz goldenAlphaRadiusBound

